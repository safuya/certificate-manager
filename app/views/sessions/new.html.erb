<h1>Certificate Manager</h1>

<%= form_tag sessions_url do %>
  <%= text_field_tag :username %>
  <%= password_field_tag :password %>
  <%= submit_tag 'ENTER', class: 'form_submit' %>
<% end %>
<div class="flex">
  <i class="fab fa-github fa-2x"></i>
  <%= link_to "Login using GitHub", '/auth/github', id: 'github' %>
</div>
